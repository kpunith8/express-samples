{
  "name": "express-app",
  "version": "1.0.0",
  "description": "Sample express application for the learning purpose",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node ./startMessage.js",
    "start": "npm-run-all --parallel open-source lint:watch test:watch start-mockapi",
    "lint": "esw webpack.config.* src --color",
    "lint:watch": "npm run lint -- --watch",
    "open-source": "babel-node ./index.js",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000 --subdomain punith",
    "share": "npm-run-all --parallel open-source localtunnel",
    "test": "mocha --reporter progress src/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "generate-mock-data": "babel-node ./generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kpunith8/express-samples.git"
  },
  "keywords": [
    "express"
  ],
  "author": "Punith K",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/kpunith8/express-samples/issues"
  },
  "homepage": "https://github.com/kpunith8/express-samples#readme",
  "dependencies": {
    "babel": "^6.23.0",
    "babel-register": "^6.24.1",
    "body-parser": "^1.17.2",
    "eslint": "^4.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-watch": "^3.1.2",
    "express": "^4.15.3",
    "mocha": "^3.5.0",
    "mongoose": "^4.11.1",
    "multer": "^1.3.0",
    "npm-run-all": "^4.0.2",
    "nsp": "^2.7.0",
    "pug": "^2.0.0-rc.2",
    "webpack": "^3.4.1",
    "webpack-dev-middleware": "^1.11.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "chalk": "^2.0.1",
    "cheerio": "^1.0.0-rc.2",
    "cross-env": "^5.0.3",
    "css-loader": "^0.28.4",
    "jsdom": "^9.8.0",
    "json-schema-faker": "^0.3.6",
    "json-server": "^0.8.22",
    "nock": "^8.1.0",
    "numeral": "^2.0.6",
    "open": "0.0.5",
    "path": "^0.12.7",
    "style-loader": "^0.18.2"
  }
}
